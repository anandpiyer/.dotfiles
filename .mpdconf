music_directory         "~/Music/beets"
playlist_directory      "~/Music/beets"
db_file                 "~/.mpd/mpd.db"
log_file                "~/.mpd/mpd.log"
pid_file                "~/.mpd/mpd.pid"
state_file              "~/.mpd/mpd.state"
sticker_file            "~/.mpd/sticker.sql"

bind_to_address         "127.0.0.1"
port                    "6600"
user                    "api"

auto_update             "yes"
auto_update_depth       "2"
follow_outside_symlinks "yes"
follow_inside_symlinks  "yes"

audio_output {
    type "osx"
    name "CoreAudio"
    mixer_type "software"
}